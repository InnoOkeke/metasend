{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.ts"
    },
    {
      "src": "/.well-known/apple-app-site-association",
      "dest": "/public/.well-known/apple-app-site-association"
    },
    {
      "src": "/.well-known/assetlinks.json",
      "dest": "/public/.well-known/assetlinks.json"
    },
    {
      "src": "/claim/(.*)",
      "status": 308,
      "headers": {
        "Location": "https://metasend-api.onrender.com/api/web/claim/$1"
      }
    },
    {
      "src": "/pay/(.*)",
      "status": 308,
      "headers": {
        "Location": "https://metasend-api.onrender.com/api/web/pay/$1"
      }
    },
    {
      "src": "/gift/(.*)",
      "status": 308,
      "headers": {
        "Location": "https://metasend-api.onrender.com/api/web/gift/$1"
      }
    },
    {
      "src": "/tip/(.*)",
      "status": 308,
      "headers": {
        "Location": "https://metasend-api.onrender.com/api/web/tip/$1"
      }
    }
  ]
}