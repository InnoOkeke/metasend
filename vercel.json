{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.ts"
    },
    {
      "src": "/.well-known/apple-app-site-association",
      "dest": "/public/.well-known/apple-app-site-association"
    },
    {
      "src": "/.well-known/assetlinks.json",
      "dest": "/public/.well-known/assetlinks.json"
    },
    {
      "src": "/claim/(.*)",
      "dest": "/api/web.ts"
    },
    {
      "src": "/pay/(.*)",
      "dest": "/api/web.ts"
    },
    {
      "src": "/gift/(.*)",
      "dest": "/api/web.ts"
    },
    {
      "src": "/gifts/(.*)",
      "dest": "/api/web.ts"
    },
    {
      "src": "/tip/(.*)",
      "dest": "/api/web.ts"
    },
    {
      "src": "/tipjar/(.*)",
      "dest": "/api/web.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.ts"
    }
  ]
}